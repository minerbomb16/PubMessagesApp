{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=PubMessagesDb.sqlite;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DataProtection": {
    "ApplicationDiscriminator": "PubMessagesApp",
    "KeyManagementOptions": {
      "KeyStorageDirectory": "keys",
      "XmlEncryptor": {
        "Type": "Microsoft.AspNetCore.DataProtection.XmlEncryption.DpapiXmlEncryptor, Microsoft.AspNetCore.DataProtection"
      }
    }
  },
  "AllowedHosts": "*"
}
